<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".MainActivity">

    <Button
            android:id="@+id/btn_test_okhttp3_execute"
            android:textAllCaps="false"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="test_okhttp3_execute"/>

    <Button
            android:id="@+id/btn_test_okhttp3_enqueue"
            app:layout_constraintTop_toBottomOf="@id/btn_test_okhttp3_execute"
            android:textAllCaps="false"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="test_okhttp3_enqueue"/>

    <Button
            android:id="@+id/btn_test_retrofit_execute"
            android:textAllCaps="false"
            app:layout_constraintTop_toBottomOf="@id/btn_test_okhttp3_enqueue"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="test_retrofit_execute"/>

    <Button
            android:id="@+id/btn_test_retrofit_enqueue"
            app:layout_constraintTop_toBottomOf="@id/btn_test_retrofit_execute"
            android:textAllCaps="false"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="test_retrofit_enqueue"/>

</android.support.constraint.ConstraintLayout>